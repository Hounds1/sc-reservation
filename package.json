{
    "name": "ska-reservation-monorepo",
    "private": true,
    "workspaces": [
      "apps/*",
      "packages/*"
    ],
    "scripts": {
      "dev:web": "npm -w apps/web run dev",
      "dev:api": "npm -w apps/api run start:dev",
      "build:web": "npm -w apps/web run build",
      "build:api": "npm -w apps/api run build",
      "lint:web": "npm -w apps/web run lint",
      "lint:api": "npm -w apps/api run lint",
      "typecheck:web": "npm -w apps/web run typecheck",
      "typecheck:api": "npm -w apps/api run typecheck"
    },
    "devDependencies": {
      "typescript": "^5.0.0"
    },
    "engines": {
      "node": ">=20.9.0"
    }
  }
  